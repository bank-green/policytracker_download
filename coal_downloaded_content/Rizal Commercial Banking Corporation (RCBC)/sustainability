						<section class="section__content m-t--lg p-b--lg">
			<div id="tabAnchor" class="container">
				<div class="row">
					<div class="col-xs-12 col-sm-12 col-md-12">
						<div class="corporate-governance tab-content">
							<div id="corporateGovernanceList" class="list-view col-md-3">

								<div class="mobile-custom-dropdown js-mobile-custom-dropdown">
									<span class="selected">Corporate Profile</span>
									<ul class="tab tab--vertical custom-dropdown-menu js-custom-dropdown-menu">
