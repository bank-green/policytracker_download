<script>
	'use strict';

	function advisoryPopup() {
		if (window.location.pathname == '/stocktransfer') {
			var _innerHtml = '<div class="advisory-content text--center">';
			_innerHtml += '  <h4>Advisory</h4>';
			_innerHtml += '  <a href="" id="closePopup" class="jq-close"></a>';
			_innerHtml += '	 <div class="content-text">';
			_innerHtml += '  	<p style="text-align: center;margin-bottom: 15px;">Should you encounter a slowdown in the downloading or uploading of your CDDCP documents, please logout and re-access the Official Stock Transfer Page (www.rcbc.com/stocktransfer) after a few hours in order to re-enroll your CDDCP application documents.</p>';
			_innerHtml += '  </div>';

			document.getElementById('advisory-wrap').innerHTML = _innerHtml;
			document.getElementById('advisory-wrap').classList.add('active');
		} else if (window.location.pathname == '/rcbc-time-deposit') {
			var _innerHtml = '<div class="advisory-content text--center" style="max-width: 510px;">';
			_innerHtml += '  <h4>Advisory</h4>';
			_innerHtml += '  <a href="" id="closePopup" class="jq-close"></a>';
			_innerHtml += '	 <div class="content-text" style="border:0px;">';
			_innerHtml += '<p>In line with our sustainability efforts, we will no longer be providing a physical or electronic copy of the Time Deposit Statement of Account starting November 2, 2023. A copy can be made available upon request. For concerns. kindly reach out to your Branch of Account or call our hotline at 8877-7222.</p>';
			_innerHtml += '  </div>';

                        document.getElementById('advisory-wrap').innerHTML = _innerHtml;
			document.getElementById('advisory-wrap').classList.add('active');
		} else if (window.location.pathname == '/rcbc-assist-online') {
			var _innerHtml = '<div class="advisory-content text--center" style="max-width: 510px;">';
			_innerHtml += '  <h4></h4>';
			_innerHtml += '  <a href="" id="closePopup" class="jq-close"></a>';
			_innerHtml += '	 <div class="content-text" style="border:0px;">';
			_innerHtml += '<h5>Touch Q is now RCBC Assist Online!</h5><p>Queue your branch transactions via our new website. <section class="tab-content__footer text--center"><a class="button--primary button--half" role="button" href="https://assist.rcbc.com" target="_blank" rel="noopener">Visit RCBC Assist Website</a></section></p>';
			_innerHtml += '  </div>';

                        document.getElementById('advisory-wrap').innerHTML = _innerHtml;
			document.getElementById('advisory-wrap').classList.add('active');
		} else if (window.location.pathname == '/online-fx') {
			var _innerHtml = '<div class="advisory-content text--center" style="max-width: 510px;">';
			_innerHtml += '  <a href="" id="closePopup" class="jq-close"></a>';
			_innerHtml += '	 <div class="content-text">';
			_innerHtml += '     <img src="https://www.rcbc.com/uploads/media/Online-FX-Advisory-V3.jpg" style="width:100%">';
			_innerHtml += '  </div>';

            document.getElementById('advisory-wrap').innerHTML = _innerHtml;
            document.getElementById('advisory-wrap').classList.add('active');
		}

	}

	function removePopup() {
		document.getElementById('advisory-wrap').classList.remove('active');
	}

	window.addEventListener('load', function () {
		advisoryPopup();

		document.getElementById('closePopup').addEventListener('click', function (e) {
			e.preventDefault();
			removePopup();
		});
	});
</script>

