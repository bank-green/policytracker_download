<script type="text/javascript">
    function ReplaceNotValidChar(keyword) {
        $.each('<,>'.split(','), function (i, e) {
            while (keyword.indexOf(e) > -1) {
                keyword = keyword.replace(e, '');
            }
        });
        return keyword;
    }
</script>

